#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define SIZE 32 //changed the size of this too a bigger value, because 16 bytes was given an error when trying 'notarealusername'.

int main () {
    char *data1;

    data1 = malloc (SIZE);
    printf ("Please input username: ");
    scanf ("%s", data1);
    printf ("you entered: %s\n", data1);
    free (data1);

    return 0;
}


